{
    "directories": ["src"],
    "files": [
        "LICENSE",
        "i18n.php",
        "vendor/symfony/translation/Resources/data/parents.json"
    ],
    "finder": [
        {
            "name": ["*.php", "*.xsd", "LICENSE"],
            "exclude": ["Tests", "tests", "sebastian", "phpunit", "phpspec", "process", "filesystem"],
            "in": "vendor"
        }
    ],
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\PhpScoper"
    ],
    "output": "behat.phar",
    "stub": "build/stub.php"
}
